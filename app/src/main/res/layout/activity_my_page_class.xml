<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/black"
    tools:context=".mypage.MyPageClass">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.widget.Button
            android:id="@+id/goback"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginVertical="15dp"
            android:background="@drawable/white_arrow"
            android:gravity="left"
            android:layout_marginLeft="10dp"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:fontFamily="@font/notosanskr_regular"
            android:gravity="center"
            android:text="수강 내역"
            android:textColor="#FFFFFF"
            android:textSize="16dp" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="10dp">

            <ImageButton
                android:id="@+id/image1"
                android:layout_width="70dp"
                android:layout_height="70dp"/>

            <LinearLayout
                android:layout_width="225dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/name1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="에스파 안무 댄스 클래스"
                    android:textColor="#FFFFFF"
                    android:textSize="14dp"
                    android:fontFamily="@font/notosanskr_medium"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:id="@+id/location1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="노원구 공릉동"
                    android:textSize="10dp"
                    android:textColor="#FFFFFF"
                    android:fontFamily="@font/notosanskr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="2023.05.12(금) 16:00 - 18:00"
                    android:textSize="10dp"
                    android:textColor="#FFFFFF"
                    android:fontFamily="@font/notosanskr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
            </LinearLayout>


            <android.widget.Button
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@drawable/mypage_arrowbutton"
                android:layout_gravity="center"
                android:gravity="center"
                />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="10dp">

            <ImageButton
                android:layout_width="70dp"
                android:layout_height="70dp"/>

            <LinearLayout
                android:layout_width="225dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="에스파 안무 댄스 클래스"
                    android:textColor="#FFFFFF"
                    android:textSize="14dp"
                    android:fontFamily="@font/notosanskr_medium"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:id="@+id/location2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="노원구 공릉동"
                    android:textSize="10dp"
                    android:textColor="#FFFFFF"
                    android:fontFamily="@font/notosanskr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="2023.05.12(금) 16:00 - 18:00"
                    android:textSize="10dp"
                    android:textColor="#FFFFFF"
                    android:fontFamily="@font/notosanskr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
            </LinearLayout>


            <android.widget.Button
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@drawable/mypage_arrowbutton"
                android:layout_gravity="center"
                android:gravity="center"
                />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="10dp">

            <ImageButton
                android:layout_width="70dp"
                android:layout_height="70dp"/>

            <LinearLayout
                android:layout_width="225dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/name3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="에스파 안무 댄스 클래스"
                    android:textColor="#FFFFFF"
                    android:textSize="14dp"
                    android:fontFamily="@font/notosanskr_medium"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:id="@+id/location3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="노원구 공릉동"
                    android:textSize="10dp"
                    android:textColor="#FFFFFF"
                    android:fontFamily="@font/notosanskr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="2023.05.12(금) 16:00 - 18:00"
                    android:textSize="10dp"
                    android:textColor="#FFFFFF"
                    android:fontFamily="@font/notosanskr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"/>
            </LinearLayout>


            <android.widget.Button
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@drawable/mypage_arrowbutton"
                android:layout_gravity="center"
                android:gravity="center"
                />
        </LinearLayout>
    </LinearLayout>


</LinearLayout>